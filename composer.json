{
  "name": "metamodels/attribute_translatedlongtext",
  "description": "MetaModels attribute for translated longtext",
  "keywords": [
    "contao",
    "metamodels",
    "attribute",
    "longtext",
    "translatable"
  ],
  "type": "contao-bundle",
  "homepage": "http://now.metamodel.me/",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Christian Schiffler",
      "email": "c.schiffler@cyberspectrum.de",
      "homepage": "http://www.cyberspectrum.de",
      "role": "Developer"
    },
    {
      "name": "Stefan Heimes",
      "email": "stefan_heimes@hotmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "mail@metamodel.me",
    "issues": "https://github.com/MetaModels/attribute_translatedlongtext/issues",
    "wiki": "http://de.contaowiki.org/MetaModels",
    "irc": "irc://irc.freenode.org/contao.mm",
    "source": "https://github.com/MetaModels/attribute_translatedlongtext"
  },
  "require": {
    "php": ">=5.6",
    "contao/core-bundle": "^4.4",
    "contao-community-alliance/dc-general": "~2.1@dev",
    "metamodels/core": "~2.1@dev",
    "symfony/dependency-injection": "^3.3",
    "symfony/http-kernel": "^3.3"
  },
  "require-dev": {
    "cyberspectrum/contao-toolbox": "~0.6",
    "phpcq/all-tasks": "~1.1",
    "contao/manager-plugin": "~2.1"
  },
  "autoload": {
    "psr-4": {
      "MetaModels\\AttributeTranslatedLongtextBundle\\": "src"
    },
    "files": [
      "src/deprecated-autoload.php"
    ]
  },
  "extra": {
    "contao-manager-plugin": "MetaModels\\AttributeTranslatedLongtextBundle\\ContaoManager\\Plugin",
    "contao": {
      "transifex": {
        "project": "metamodels",
        "prefix": "attribute_translatedlongtext-",
        "languages_cto": "src/Resources/contao/languages",
        "languages_tx": ".tx"
      }
    },
    "branch-alias": {
      "dev-master": "2.0.x-dev",
      "dev-feature/contao4": "2.1.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "git@gitlab.com:metamodels/core.git"
    }
  ]
}
